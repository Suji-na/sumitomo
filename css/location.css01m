@charset "utf-8";

.breadcrumb {
    padding: 20px 0;
    border-top: 1px solid #f1e9dc;
    border-bottom: 1px solid #f1e9dc;
}

.breadcrumb ul {
    display: flex;
    flex-wrap: wrap;
    font-size: 1.4rem;
    font-weight: 300;
    letter-spacing: .02em;
    color: #888;
}

.breadcrumb ul li a {
    color: #000;
}

.breadcrumb ul li:not(:last-child) a::after,
.breadcrumb ul li:not(:last-child) p::after {
    content: ' ï¼ž ';
    display: inline-block;
}

.location-link {
    display: flex;
    flex-wrap: wrap;
}

.location-link a {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    height: 100px;
    padding: 10px 2.5vw;
    font-size: 1.8rem;
    font-weight: 300;
    letter-spacing: .1em;
    background-color: #fefcf7;
    border-left: 1px solid #f1e9dc;
    border-bottom: 1px solid #f1e9dc;
}

.location-link a.current {
    color: #fff;
    background-color: #868076;
}

.location-link--3 a {
    width: 33.33%;
}

.location-link--4 a {
    width: 25%;
}

.location-link--5 a {
    width: 20%;
}

.location-link a::before {
    content: '\f105';
    position: absolute;
    left: calc(2.5vw - 15px);
    top: 50%;
    font-family: 'Font Awesome 6 Free';
    font-weight: 600;
    color: #808081;
    transform: translateY(-50%);
}

.location-link a.current::before {
    color: #fff;
}

.caption {
    font-family: 'Noto Sans JP', sans-serif;
    font-size: 10px;
}

@media screen and (max-width: 1400px) {
    .location-link a {
        font-size: 1.28vw;
    }
}

@media screen and (max-width: 768px) {
    .location-link a {
        padding: 10px 25px;
        font-size: 1.75vw;
    }

    .location-link--4 a {
        width: 50%;
    }

    .location-link--5 a {
        width: 33.33%;
    }

    .location-link a::before {
        left: 10px;
    }
}

@media screen and (max-width: 639px) {
    .location-link a {
        width: 100% !important;
        height: 13.33vw;
        font-size: 4vw;
    }

    .location-link a::before {
        left: 6vw;
    }

    .caption {
        max-width: 100%;
    }

}


/* NEW LOCATION CODING */
.location {
    font-family: "Shippori Mincho", serif;

    img {
        display: block;
    }
}

.location .p-section {
    padding-block: 60px;
}

.location .p-container {
    max-width: 1350px;
    margin-inline: auto;
    padding-inline: 20px;
}

.location .top-mv {
    position: relative;
}

.location .top-mv-txt {
    position: absolute;
    color: #FFF;
    font-family: "Cormorant Infant", serif;
    font-size: 90px;
    line-height: 1;
    letter-spacing: .092em;
    bottom: -15px;
    left: 50px;
}

.location .top-mv-note {
    color: #FFF;
    position: absolute;
    font-family: "Zen Kaku Gothic New", sans-serif;
    font-weight: 500;
    font-size: 12px;
    line-height: 21px;
    bottom: 10px;
    left: 10px;
}


/* DESCRIPTION SECTION */
.location .description {
    background-color: #F5F5F5;
    padding-block: 100px;
}

.location .description-ttl {
    font-size: 32px;
    line-height: 40px;
}

.location .description-desc {
    font-size: 18px;
    line-height: 30px;
    margin-block: 60px;
}

.location .description-img {
    padding-inline: 85px;
    position: relative;
}

.location .description-txt {
    font-family: "Zen Kaku Gothic New", sans-serif;
    position: absolute;
    font-size: 12px;
    line-height: 21px;
    right: 108px;
    bottom: 5px;
}

.location .description-note {
    font-family: "Zen Kaku Gothic New", sans-serif;
    font-size: 12px;
    line-height: 21px;
    margin-top: 52px;
    padding-inline: 85px;
}


/* SELECT SECTION */
.select.bg-gray {
    background-color: #F5F5F5;
}

.select-block {
    max-width: 1200px;
    width: 100%;
    padding-inline: 20px;
    margin-inline: auto;

    display: flex;
    align-items: center;
    gap: 40px;
}

.select-link {
    position: relative;
    width: 100%;
    font-family: "Cormorant Infant", serif;
    border-bottom: 2px solid #DDDDDD;
    text-align: center;

    font-size: 25px;
    line-height: 30px;
    letter-spacing: .08em;
    padding-bottom: 23px;
    margin-bottom: 178px;
}

.select-link.active {
    border-color: #000;
}

.select-link:after {
    content: '';
    position: absolute;
    border-width: 0 1px 1px 0;
    border-color: transparent #000 #000 transparent;
    border-style: solid;
    transform: rotate(45deg) translateX(-50%);
    padding: 7px;
    bottom: -45px;
    left: 50%;
}

/* COMMON SECTION */
.location .section-ttl {
    font-family: "Cormorant Infant", serif;
    font-size: 75px;
    line-height: 91.84px;
    letter-spacing: .16em;
}

.location .section-sub {
    font-size: 32px;
    line-height: 58px;
    margin-block: 70px 35px;
}

.location .section-desc {
    font-size: 20px;
    line-height: 46px;
    margin-bottom: 96px;
}

.location .section-note {
    font-family: "Zen Kaku Gothic New", sans-serif;
    font-size: 12px;
    line-height: 21px;
}

