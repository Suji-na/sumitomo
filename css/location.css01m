.area {
    padding-block: 115px 230px;
}

.area-ttl {
    cursor: pointer;
    background-color: #333;
    color: #FFF;
    display: flex;
    justify-content: center;
    /* justify-content: flex-end; */
    font-family: "Cormorant Infant", serif;
}

.area-ttl p {
    width: 100%;
    text-align: center;
    font-size: 30px;
    line-height: 91.84px;
    letter-spacing: .16em;
}

.area-ttl span {
    position: relative;
    margin-inline: auto 0;
    font-size: 15px;
    line-height: 91.84px;
    letter-spacing: .16em;
    padding-right: 77px;
    max-width: 130px;
    width: 100%;
}

.area-ttl span:after {
    position: absolute;
    content: '';
    border-width: 0 1px 1px 0;
    border-color: transparent #FFF #FFF transparent;
    border-style: solid;
    transform: rotate(224deg) translateY(-50%);
    padding: 7px;
    top: 40%;
    left: 75px;
}

.area-ttl span:after .rotate {
    transform: rotate(224deg) translateY(-50%);
}

.area-content.active {
    max-height: 0;
    opacity: 0;
    visibility: hidden;
    transition: all .4s ease-in-out;
}

.area-content.active .close {
    display: none;
}

.area-content-img {
    margin-block: 45px 7px;
}

.area-row {
    display: flex;
    gap: 60px;
    margin-top: 36px;
}

.area-col {
    width: 100%;
    border-top: 1px solid #cfcbcb;
}

.area-block {
    width: 100%;
}

.area-col h5 {
    font-size: 18px;
    line-height: 31.5px;
    padding-block: 13px 9px;
}

.area-col li {
    display: flex;
    justify-content: space-between;
    gap: 7px;
}

.area-col .txt {
    display: flex;
    align-items: center;
    gap: 5px;
    font-size: 18px;
    line-height: 33.5px;
}

.area-col .num {
    font-size: 18px;
    line-height: 33.5px;
}

.area-col.mb50 {
    margin-bottom: 46px;
}

.area-col .txt span {
    display: inline-block;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #FFF;
    width: 17.42px;
    height: 17.42px;
    border-radius: 20px;
    font-size: 14px;
    line-height: 33.5px;
    letter-spacing: -.1em;
}

.area-col .txt .red {
    background-color: #C13C3C;
}

.area-col .txt .blue {
    background-color: #97BDDC;
}

.area-col .txt .ylw {
    background-color: #D3A51A;
}

.area-col .txt .dblue {
    background-color: #306A9F;
}

.area-col .txt .green {
    background-color: #86CB67;
}

.area-col .txt .black {
    background-color: #333;
}

.section-note.m-block {
    margin-block: 80px 33px;
}

@media (max-width:767px) {
    .area {
        padding-block: 45px 150px;
    }

    .area-ttl {
        padding-inline: 20px 35px;
    }

    .area-ttl p {
        text-align: left;
        font-size: 14px;
        line-height: 47px;
    }

    .area-ttl span {
        max-width: fit-content;
        width: 100%;
        font-size: 12px;
        line-height: 47px;
        padding-right: 0px;
    }

    .area-ttl span:after {
        position: absolute;
        content: '';
        border-width: 0 1px 1px 0;
        border-color: transparent #FFF #FFF transparent;
        border-style: solid;
        transform: rotate(224deg) translateY(-50%);
        padding: 4px;
        top: 40%;
        right: -22px;
        left: unset;
    }

    .area-content-img {
        margin-block: 20px 5px;
    }

    .area-row {
        flex-direction: column;
        gap: 30px;
        margin-top: 30px;
    }

    .area-col h5 {
        font-size: 15.8px;
        line-height: 27.64px;
        padding-block: 14px 12px;
    }

    .area-col li {
        gap: 7px;
    }

    .area-col .txt {
        gap: 5px;
        font-size: 15.8px;
        line-height: 29.4px;
    }

    .area-col .num {
        font-size: 15.8px;
        line-height: 29.4px;
    }

    .area-col.mb50 {
